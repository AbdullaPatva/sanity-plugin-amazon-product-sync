{
  "name": "@multidots/sanity-plugin-amazon-product-sync",
  "version": "1.0.6",
  "description": "Sanity Studio plugin to fetch and embed Amazon products by ASIN (PA-API v5), with settings tool and document actions.",
  "keywords": [
    "sanity",
    "sanity-plugin",
    "amazon",
    "asin",
    "product advertising api",
    "ecommerce"
  ],
  "license": "MIT",
  "author": "Multidots <sanity@multidots.com>",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "import": "dist/index.js",
      "default": "dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "sideEffects": false,
  "browserslist": "extends @sanity/browserslist-config",
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "examples",
    "src",
    "screenshots",
    "eslint.config.js",
    "tsconfig.json"
  ],
  "scripts": {
    "build": "plugin-kit verify-package --silent && pkg-utils build --strict --check --clean",
    "watch": "pkg-utils watch --strict",
    "link-watch": "plugin-kit link-watch",
    "prepublishOnly": "npm run build",
    "dev": "pkg-utils watch --strict",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18",
    "sanity": ">=3.86.0 <5"
  },
  "dependencies": {
    "@sanity/client": "^6.12.0",
    "@sanity/ui": "^2.6.0",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@sanity/browserslist-config": "^1.0.5",
    "@sanity/pkg-utils": "^7.11.8",
    "@sanity/plugin-kit": "^4.0.0",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^8.39.1",
    "@typescript-eslint/parser": "^8.39.1",
    "typescript": "^5.5.4"
  },
  "engines": {
    "node": ">=18"
  },
  "sanityPlugin": {
    "verifyPackage": {
      "eslintImports": false,
      "sanityV2Json": false
    }
  }
}
